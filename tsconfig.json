{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
      "target": "ES2019",
      "baseUrl": ".",
      "jsx": "preserve",
      "paths": {
          "src/*": ["src/*"],
          "app/*": ["*"],
          "components/*": ["src/components/*"],
          "layouts/*": ["src/layouts/*"],
          "pages/*": ["src/pages/*"],
          "assets/*": ["src/assets/*"],
          "boot/*": ["src/boot/*"],
          "setup/*": ["src/setup/*"],
          "data/*": ["src/randomizer/*"]
      }
  },
  "include": [
      "src/**/*.ts",
      "src/**/*.vue"
  ]
}
